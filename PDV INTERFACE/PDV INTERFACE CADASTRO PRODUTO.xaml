<Window x:Class="PDV_INTERFACE_CADASTRO_PRODUTO.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:PDV_INTERFACE_CADASTRO_PRODUTO"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <Button x:Name="buttonSalvar" Content="SALVAR" Margin="315,334,315,0" VerticalAlignment="Top" Height="36" Click="buttonSalvar_Click" RenderTransformOrigin="0.5,0.5" FontWeight="Bold">
            <Button.RenderTransform>
                <TransformGroup>
                    <ScaleTransform/>
                    <SkewTransform/>
                    <RotateTransform Angle="-0.227"/>
                    <TranslateTransform X="0.372" Y="-0.001"/>
                </TransformGroup>
            </Button.RenderTransform>
        </Button>
        <TextBox x:Name="textBoxPrecoVenda" Margin="319,296,319,0" TextWrapping="Wrap" VerticalAlignment="Top" Height="31" TextChanged="TextBox_TextChanged" RenderTransformOrigin="0.5,0.5">
            <TextBox.RenderTransform>
                <TransformGroup>
                    <ScaleTransform/>
                    <SkewTransform/>
                    <RotateTransform Angle="-0.227"/>
                    <TranslateTransform X="0.254" Y="0.009"/>
                </TransformGroup>
            </TextBox.RenderTransform>
        </TextBox>
        <TextBox x:Name="textBoxPrecoCompra" HorizontalAlignment="Center" Margin="0,234,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="162" Height="30" RenderTransformOrigin="0.5,0.5" TextChanged="TextBoxPrecoCompra_TextChanged">
            <TextBox.RenderTransform>
                <TransformGroup>
                    <ScaleTransform/>
                    <SkewTransform/>
                    <RotateTransform Angle="-0.227"/>
                    <TranslateTransform X="0.083" Y="0.01"/>
                </TransformGroup>
            </TextBox.RenderTransform>
        </TextBox>
        <TextBox x:Name="textBoxCodigoProduto" HorizontalAlignment="Center" Margin="0,166,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="162" Height="32" RenderTransformOrigin="0.5,0.5" TextChanged="TextBoxCodigoProduto_TextChanged">
            <TextBox.RenderTransform>
                <TransformGroup>
                    <ScaleTransform/>
                    <SkewTransform/>
                    <RotateTransform Angle="-0.227"/>
                    <TranslateTransform X="-0.103" Y="0.01"/>
                </TransformGroup>
            </TextBox.RenderTransform>
        </TextBox>
        <TextBox x:Name="textBoxNomeProduto" HorizontalAlignment="Center" Margin="0,100,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="162" Height="31" RenderTransformOrigin="0.5,0.5" TextChanged="TextBoxNomeProduto_TextChanged">
            <TextBox.RenderTransform>
                <TransformGroup>
                    <ScaleTransform/>
                    <SkewTransform/>
                    <RotateTransform Angle="-0.227"/>
                    <TranslateTransform X="-0.277" Y="0.01"/>
                </TransformGroup>
            </TextBox.RenderTransform>
        </TextBox>
        <Label Content="Nome do Produto" HorizontalAlignment="Center" Margin="0,70,0,0" VerticalAlignment="Top" Width="162" FontSize="16" RenderTransformOrigin="0.5,0.5" Height="31" FontWeight="Bold">
            <Label.RenderTransform>
                <TransformGroup>
                    <ScaleTransform/>
                    <SkewTransform/>
                    <RotateTransform Angle="-0.227"/>
                    <TranslateTransform X="-0.38" Y="0.011"/>
                </TransformGroup>
            </Label.RenderTransform>
        </Label>
        <Label x:Name="___Sem_Nome_" Content="Código do Produto" HorizontalAlignment="Center" Margin="0,135,0,0" VerticalAlignment="Top" Width="162" FontSize="16" RenderTransformOrigin="0.5,0.5" Height="32" FontWeight="Bold">
            <Label.RenderTransform>
                <TransformGroup>
                    <ScaleTransform/>
                    <SkewTransform/>
                    <RotateTransform Angle="-0.227"/>
                    <TranslateTransform X="-0.186" Y="0.01"/>
                </TransformGroup>
            </Label.RenderTransform>
        </Label>
        <Label x:Name="___Sem_Nome__Copiar" Content="Preço de Compra" HorizontalAlignment="Center" VerticalAlignment="Center" Width="162" FontSize="16" RenderTransformOrigin="0.5,0.5" Height="31" FontWeight="Bold">
            <Label.RenderTransform>
                <TransformGroup>
                    <ScaleTransform/>
                    <SkewTransform/>
                    <RotateTransform Angle="-0.227"/>
                    <TranslateTransform X="-0.007" Y="0.01"/>
                </TransformGroup>
            </Label.RenderTransform>
        </Label>
        <Label x:Name="___Sem_Nome__Copiar1" Content="Preço de Venda" Margin="319,265,319,0" VerticalAlignment="Top" FontSize="16" RenderTransformOrigin="0.5,0.5" Height="31" FontWeight="Bold">
            <Label.RenderTransform>
                <TransformGroup>
                    <ScaleTransform/>
                    <SkewTransform/>
                    <RotateTransform Angle="-0.227"/>
                    <TranslateTransform X="0.163" Y="0.01"/>
                </TransformGroup>
            </Label.RenderTransform>
        </Label>
        <Label Content="CADASTRO DO PRODUTO" HorizontalAlignment="Center" Margin="0,36,0,0" VerticalAlignment="Top" Width="230" FontSize="16" RenderTransformOrigin="0.5,0.5" Height="31" FontWeight="Bold" FontStretch="Normal" FontFamily="Arial Black">
            <Label.RenderTransform>
                <TransformGroup>
                    <ScaleTransform/>
                    <SkewTransform/>
                    <RotateTransform Angle="-0.227"/>
                    <TranslateTransform X="-0.38" Y="0.011"/>
                </TransformGroup>
            </Label.RenderTransform>
        </Label>
        <Button x:Name="buttonLimpar" Content="LIMPAR" HorizontalAlignment="Left" Margin="506,337,0,0" VerticalAlignment="Top" Height="34" Width="89" Click="buttonLimpar_Click"/>

    </Grid>
</Window>
